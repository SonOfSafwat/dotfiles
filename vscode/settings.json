{
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.showUnused": true
  },
  "code-runner.clearPreviousOutput": true,
  "code-runner.enableAppInsights": false,
  "css.fileExtensions": ["css", "scss"],
  "css.lint.duplicateProperties": "error",
  "css.lint.emptyRules": "error",
  "css.lint.float": "error",
  "css.lint.idSelector": "error",
  "css.lint.ieHack": "error",
  "css.lint.important": "error",
  "diffEditor.renderSideBySide": true,
  "editor.autoIndent": "full",
  "editor.codeActionsOnSave": {
    // For ESLint
    "source.fixAll.eslint": true,
    // For TSLint
    "source.fixAll.tslint": true,
    // For Stylelint
    "source.fixAll.stylelint": true
  },
  "editor.codeLens": true,
  "editor.cursorBlinking": "solid",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorStyle": "line",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'MesloLGS NF', 'SourceCodePro+Powerline+Awesome Regular',Source Code Pro for Powerline",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.renameOnType": true,
  "editor.renderControlCharacters": true,
  "editor.renderWhitespace": "all",
  "editor.suggest.statusBar.visible": true,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.tabSize": 2,
  "editor.wordWrap": "off",
  "errorLens.addAnnotationTextPrefixes": true,
  "errorLens.fontStyleItalic": true,
  "errorLens.gutterIconsEnabled": true,
  "eslint.alwaysShowStatus": true,
  "eslint.format.enable": true,
  "eslint.lintTask.enable": true,
  "explorer.compactFolders": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.autoGuessEncoding": true,
  "files.exclude": {
    "**/.git": true,
    "**/.DS_Store": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/*.js.map": {
      "when": "$(basename)"
    }
  },
  "files.hotExit": "onExit",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "git.fetchOnPull": true,
  "gitProjectManager.baseProjectsFolders": [
    "/Users/abdelrhmansafwat/Documents/projects"
  ],
  "gitProjectManager.displayProjectPath": true,
  "gitProjectManager.maxDepthRecursion": 0,
  "gitProjectManager.storeRepositoriesBetweenSessions": true,
  "html.format.indentHandlebars": true,
  "html.format.indentInnerHtml": true,
  "html.format.wrapAttributes": "force-aligned",
  "html.format.wrapLineLength": 80,
  "i18n-ally.editor.preferEditor": true,
  "i18n-ally.namespace": true,
  "i18n-ally.pathMatcher": "{namespaces}/{locale}.{ext}",
  "i18n-ally.sortKeys": true,
  "javascript.implicitProjectConfig.checkJs": true,
  "javascript.preferences.quoteStyle": "single",
  "javascript.referencesCodeLens.enabled": true,
  "javascript.referencesCodeLens.showOnAllFunctions": true,
  "javascript.suggest.completeFunctionCalls": true,
  "javascript.validate.enable": false,
  "json.schemas": [
    {
      "fileMatch": ["/.babelrc"],
      "url": "http://json.schemastore.org/babelrc"
    },
    {
      "fileMatch": ["/.angular-cli.json"],
      "url": "https://raw.githubusercontent.com/angular/angular-cli/master/packages/angular/cli/lib/config/schema.json"
    },
    {
      "fileMatch": ["/appsettings.json"],
      "url": "https://json.schemastore.org/appsettings"
    },
    {
      "fileMatch": ["/bower.json"],
      "url": "https://json.schemastore.org/bower"
    },
    {
      "fileMatch": ["/.bowerrc"],
      "url": "https://json.schemastore.org/bowerrc"
    },
    {
      "fileMatch": ["/cspell"],
      "url": "https://raw.githubusercontent.com/streetsidesoftware/cspell/master/cspell.schema.json"
    },
    {
      "fileMatch": ["/docfx.json"],
      "url": "https://json.schemastore.org/docfx"
    },
    {
